{
  "name": "multipart-fetch",
  "version": "0.2.0",
  "description": "Separate the parts of a multipart stream",
  "keywords": [
    "client",
    "fetch",
    "response",
    "multipart",
    "parse",
    "parser",
    "stream"
  ],
  "author": "Rahul Gupta",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CxRes/multipart-fetch.git"
  },
  "type": "module",
  "main": "src/index.js",
  "exports": "./src/index.js",
  "browser": "dist/browser.min.js",
  "unpkg": "dist/browser.min.js",
  "types": "src/index.d.ts",
  "dependencies": {
    "@sec-ant/readable-stream": "^0.6.0",
    "content-type": "^1.0.5",
    "streamsearch-web": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@eslint/markdown": "^7.5.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "@vitest/ui": "^4.0.18",
    "cspell": "^9.6.4",
    "dedent": "^1.7.1",
    "esbuild": "0.27.3",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-regexp": "^3.0.0",
    "globals": "^17.3.0",
    "headright": "^0.3.0",
    "husky": "^9.1.7",
    "knip": "^5.85.0",
    "lint-staged": "^16.2.7",
    "parse-gitignore": "^2.0.0",
    "prettier": "^3.8.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">20.6"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "format": "prettier * --check",
    "header": "hr --dry --verbose \"**\"",
    "header:modify": "hr --verbose \"**\"",
    "lint": "pnpm run --reporter=append-only --aggregate-output \"/(^lint:.*|format)/\"",
    "lint:code": "eslint .",
    "lint:header": "hr --dry \"**\"",
    "lint:knip": "knip",
    "lint:packages": "pnpm dedupe --check",
    "lint:spelling": "cspell --dot \"**\"",
    "prepare": "node .husky/install.js",
    "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  }
}
